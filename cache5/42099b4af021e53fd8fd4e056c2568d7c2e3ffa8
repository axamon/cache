				$("form[action='https://auth.tim.it/dcauth/faces/dcauth/pages/authentication/genericlogin/genericlogin.xhtml']").submit(function(e){
					
					$(".emptyLoginError").remove();
					
					var $theForm = $(this);
					var result = true;
					
					var inputsVisible = $("input:visible", $theForm);
					
					inputsVisible.each(function(){
						var $thisInput = $(this);
						
						if(!$thisInput.is(":checkbox")){
							if($thisInput.val() == ""){
								var errorMsg='<p class="red_error lungo emptyLoginError" id="emptyLogin"><strong>Il campo ' + $thisInput.attr("name") + ' non pu&ograve; essere vuoto.</strong></p>';
								$thisInput.after(errorMsg);
								if(result)
									result = false;
							}
						}
					});
					
					if( result==false)return result;					
		
					//DCA check -i
					inputsVisible.each(function(){
						var $thisInput = $(this);
						if($thisInput.attr("name") == "password"){
						
