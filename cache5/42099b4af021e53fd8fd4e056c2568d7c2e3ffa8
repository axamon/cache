	<script>
		$(document).ready(function(){
			$('#modal_offerta').modal({keyboard:false,backdrop:'static'});
			
			$('#bottone_modale').click(function(){
				impostaCookie('modaleOfferta','true','');
				$('#modal_offerta').modal('backdrop: false');
			});
			
			function impostaCookie (nome, valore, scadenza) {
				 if (scadenza == "") {
			        var oggi = new Date();
			        oggi.setMonth(oggi.getMonth() + 120);
			        //restituisce la data nel formato necessario
			        scadenza = oggi.toGMTString();
			    }
			    valore = escape(valore);
			    document.cookie=nome + "=" + valore + ";expires=" + scadenza;
			}
		});	
	</script>

		
	
