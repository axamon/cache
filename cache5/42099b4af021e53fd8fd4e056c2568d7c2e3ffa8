        var _adftrack = {
            pm: 307530,
            divider: encodeURIComponent("|"),
            pagename: encodeURIComponent("Homepage")
        };
        
        function putim_adftrack(pagename){
          if(typeof(adf)!='undefined' && typeof(_adftrack.order)!='undefined'){
            adf.track(307530, pagename, _adftrack.order);
          }
        }
        
        function putim_adftrack_wcb(context){
          if(typeof(adf)!='undefined' && typeof(_adftrack.order)!='undefined'){
            adf.track(307530, 'WCB|'+context, $.extend({}, {sv2: context}, _adftrack.order));
          }
        }
      
        function showWebCallback(id, context, visualizzaPaginaSeparata) {
        	if (visualizzaPaginaSeparata) {
        		window.location.href = '/browse/webcallback.jsp?itemId=' + id + '&context=' + context;
        	} else {
                if($('#modal_msg_'+id+'_'+context).first().find('iframe').length == 0){
                    $('#modal_msg_'+id+'_'+context).modal('show');
                }
                else{
                    $('#modal_msg_'+id+'_'+context+' iframe').one('load', function() {
                        var $popup = $(this).closest('div[id^="modal_msg_"]');
                        $popup.modal('show');
                        putim_adftrack_wcb($popup.attr('rel'));
                    });
                    $('#modal_msg_'+id+'_'+context).first().find('iframe').attr('src', function ( i, val ) { return val; });
                }
            }
        }
    </script>
        
		
        
            
                
            
        
        
      
                
        
        
        
        	
	        
	        	
					
					
				
					
					
						
