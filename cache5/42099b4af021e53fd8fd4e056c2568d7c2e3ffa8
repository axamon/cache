		<script type="text/javascript">
			$(function(){
				$('.slider_home li[rel^="proponi"]:not(.clicked) a').click(function(e){
					var elem = $(this).closest('li[rel^="proponi"]');
					if(!elem.hasClass('clicked')){
						var anchor = $(this);
						e.preventDefault();
						elem.addClass('clicked');
						var idProponi = elem.attr('rel').replace('proponi-','');
						$.ajax({
				            url: '/global/ajax/proponi.jsp?azione=click&idProponi=' + idProponi,
				            dataType: 'text',
				            timeout: 2000,
				            success: function (data) {
				            	anchor[0].click();
				            },
				            error: function (data) {
				            	anchor[0].click();
				            }
				        });
					}
				});
			});
		</script>
