		<script type="text/javascript">
						
			$( document ).ready(function() {
				var wrongCliError = "Il numero inserito non \u00E8 corretto";
				$(document).on('submit','#form-connectivity187',function(e){
					var $inputCli = $("input[name='cli']", $(this));
					
					var formGroup = $inputCli .parents(".form-group");
					formGroup.removeClass("has-error");
					
					// cerco l'eventuale vecchio messaggio e lo rimuovo
					var errorMessage = formGroup.find(".red-error");
					errorMessage.remove();
					
					var cli = $inputCli.val();
					if ($inputCli.val().length==0 || cli != '0774571421'){
						showFormError($inputCli, wrongCliError);
						e.preventDefault();
					}
				});
			});
			
		</script>
	
	


